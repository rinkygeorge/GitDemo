package Assignments;

public class Division extends Addition {
	int result;
	
	public int sum(){
		return result;

	}
	public int getresult(){
		int sum = super.sum();
		divisible(sum);
		
	}
	public boolean divisible(int sum){
		if(sum%10==0){
		return true;
		}
		else{
		return false;
		}
		
	}

	public static void main(String[] args) {
		Division d1 = new Division();
		d1.getresult();
		System.out.println("Divisible by 10: "+d1.divisible());
		
	}

}
